library(tidyverse)
#### functions ####
parab <- function(x, a, b, c){
# y = a * (x - b)^2 + c
y = a*(x^2) + b*x + c
return(y)
}
x = seq(1,50,1)
a = seq(-.01,-.03,-.01)*10
b = seq(5,15,5)
c = seq(5,15,5)
temp <- tibble(x_val = rep(x, length(a) * length(b) * length(c)),
a_val = rep(a, each = length(x) * length(b) * length(c)),
b_val = rep(rep(b, each = length(x) * length(c)), length(a)),
c_val = rep(rep(c, each = length(x)), length(b) * length(a)))
temp %>%
mutate(y = parab(x_val, a_val, b_val, c_val),
y = ifelse(y <= 0, 0, y),
a_val = as.factor(a_val)) %>%
ggplot(aes(x_val, y,
colour = a_val)) +
geom_point() +
facet_grid(b_val~c_val)
